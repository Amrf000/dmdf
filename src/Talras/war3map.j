// TESTINGS
function LoopTest2 takes nothing returns nothing
    local integer i= 0
    loop
        set i=i + 1
        call TriggerSleepAction(1.0)
        call DisplayTextToPlayer(Player(0), 0.0, 0.0, "Waited 1 second.")
        exitwhen ( i == 10 )
    endloop
endfunction

function LoopTest takes nothing returns nothing
    local integer i= 0
    call DisplayTextToPlayer(Player(0), 0.0, 0.0, "Loop Test")
    loop
        set i=i + 1
        call LoopTest2()
        exitwhen ( i == 10 )
    endloop

endfunction

function LoopTest3 takes nothing returns boolean
    local integer i= 0
    loop
        set i=i + 1
        call TriggerSleepAction(1.0)
        call DisplayTextToPlayer(Player(0), 0.0, 0.0, "Waited 1 second.")
        exitwhen ( i == 10 )
    endloop
    return true
endfunction

function SecondLoopTest takes nothing returns nothing
    local integer i= 0
    call DisplayTextToPlayer(Player(0), 0.0, 0.0, "Second Loop Test")
    loop
        set i=i + 1
        call LoopTest3()
        exitwhen ( i == 10 )
    endloop

endfunction

